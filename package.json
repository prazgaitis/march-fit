{
  "name": "march-fit",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "author": {
    "url": "https://march.fit",
    "name": "March Fit"
  },
  "scripts": {
    "turbo": "turbo",
    "build": "turbo build --cache-dir=.turbo",
    "clean": "git clean -xdf node_modules dist .next",
    "clean:workspaces": "turbo clean",
    "dev": "bash scripts/dev.sh",
    "convex:start": "docker compose -f docker-compose.convex.yml up -d",
    "convex:stop": "docker compose -f docker-compose.convex.yml down",
    "convex:logs": "docker compose -f docker-compose.convex.yml logs -f",
    "convex:admin-key": "docker compose -f docker-compose.convex.yml exec backend ./generate_admin_key.sh",
    "format": "turbo format --cache-dir=.turbo --continue -- --cache --cache-location=\"node_modules/.cache/.prettiercache\" --ignore-path=\"../../.gitignore\"",
    "format:fix": "turbo format --cache-dir=.turbo --continue -- --write --cache --cache-location=\"node_modules/.cache/.prettiercache\" --ignore-path=\"../../.gitignore\"",
    "lint": "turbo lint --cache-dir=.turbo --affected --continue -- --cache --cache-location \"node_modules/.cache/.eslintcache\" && manypkg check",
    "lint:fix": "turbo lint --cache-dir=.turbo --affected --continue -- --fix --cache --cache-location \"node_modules/.cache/.eslintcache\" && manypkg fix",
    "typecheck": "turbo typecheck --affected --cache-dir=.turbo",
    "precommit": "turbo lint typecheck --cache-dir=.turbo --affected --continue",
    "test": "turbo test --cache-dir=.turbo",
    "fix:workspaces": "manypkg fix",
    "syncpack:list": "pnpm dlx syncpack list-mismatches",
    "syncpack:fix": "pnpm dlx syncpack fix-mismatches",
    "db:seed": "pnpm -F backend reset",
    "db:seed:if-missing": "pnpm -F backend seed:if-missing",
    "db:export": "pnpm -F backend exec npx convex export --path ${CONVEX_EXPORT_PATH:-./scripts/seed-data/export.zip}",
    "db:import": "pnpm -F backend exec npx convex import ${CONVEX_EXPORT_PATH:-./scripts/seed-data/export.zip} --replace-all -y",
    "strava:webhook": "dotenv -e .env.local -- npx tsx scripts/strava-webhook.ts",
    "storybook": "pnpm -F web storybook",
    "mf": "bun ./packages/cli/bin/mf.js",
    "mf:install": "bash scripts/install-mf.sh",
    "challenge:create": "bash scripts/create-challenge.sh",
    "challenge:create:prod": "bash scripts/create-challenge.sh --prod",
    "challenge:delete": "bash scripts/delete-challenge.sh",
    "challenge:delete:prod": "bash scripts/delete-challenge.sh --prod",
    "prepare": "husky"
  },
  "packageManager": "pnpm@10.14.0",
  "pnpm": {
    "overrides": {
      "react-is": "19.2.4",
      "require-in-the-middle": "7.5.2"
    }
  },
  "devDependencies": {
    "@convex-dev/better-auth": "^0.10.6",
    "@manypkg/cli": "^0.25.1",
    "@turbo/gen": "^2.5.6",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "turbo": "2.5.6",
    "typescript": "^5.9.2"
  }
}
